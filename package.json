{
  "name": "machinepack-postgresql",
  "version": "1.0.0",
  "description": "Run queries against a Postgresql database.",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha --recursive",
    "fasttest": "node ./node_modules/mocha/bin/mocha --recursive",
    "pretest": "npm run lint",
    "lint": "eslint machines test"
  },
  "keywords": [
    "Postgresql",
    "machines",
    "machinepack"
  ],
  "author": "Cody Stoltman <particlebanana@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "machine": "10.4.0",
    "machinepack-sql-builder": "particlebanana/waterline-query-builder",
    "pg": "4.4.6"
  },
  "devDependencies": {
    "eslint": "1.10.3",
    "espree": "3.0.1",
    "mocha": "2.3.4"
  },
  "machinepack": {
    "friendlyName": "Postgresql",
    "machineDir": "machines/",
    "machines": [
      "begin-transaction",
      "commit-transaction",
      "compile-query",
      "execute-query",
      "get-connection",
      "release-connection",
      "rollback-transaction",
      "run-query",
      "send-native-query",
      "send-query"
    ]
  }
}
